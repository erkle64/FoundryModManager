<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TitleBackground" Color="#555"/>
    <SolidColorBrush x:Key="TitleForeground" Color="White"/>
    <SolidColorBrush x:Key="WindowBackground" Color="#333"/>
    <SolidColorBrush x:Key="NormalBackground" Color="Black"/>
    <SolidColorBrush x:Key="NormalForeground" Color="White"/>
    <SolidColorBrush x:Key="DisabledBackground" Color="Black"/>
    <SolidColorBrush x:Key="DisabledForeground" Color="DarkGray"/>
    <SolidColorBrush x:Key="HighlightColor" Color="DarkBlue"/>
    <SolidColorBrush x:Key="PressedColor" Color="Blue"/>
</ResourceDictionary>